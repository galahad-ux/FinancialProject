<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="280.0" prefWidth="443.0" style="-fx-background-color: lavender;" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.isep.financialproject.AddAssestController">
   <children>
      <Label layoutX="71.0" layoutY="60.0" text="Symbol" />
      <Label layoutX="78.0" layoutY="104.0" text="Type" />
      <Label layoutX="47.0" layoutY="143.0" text="Company name" />
      <Label layoutX="67.0" layoutY="182.0" text="Quantity" />
      <Label layoutX="161.0" layoutY="23.0" text="Add your Asset (´▽｀) " />
      <Button fx:id="AddButtom" layoutX="191.0" layoutY="225.0" mnemonicParsing="false" onAction="#addAsset" stylesheets="@buttonDesign.css" text="Add" />
      <TextField fx:id="SymbolField" layoutX="192.0" layoutY="56.0" />
      <TextField fx:id="nameField" layoutX="192.0" layoutY="139.0" />
      <TextField fx:id="QuantityField" layoutX="192.0" layoutY="178.0" />
      <MenuButton fx:id="AssestType" layoutX="191.0" layoutY="100.0" mnemonicParsing="false" prefHeight="23.0" prefWidth="162.0" text="AssetType">
        <items>
          <MenuItem fx:id="Stock" mnemonicParsing="false" onAction="#chooseStock" text="Stock" />
          <MenuItem fx:id="Token" mnemonicParsing="false" onAction="#chooseToken" text="Token" />
